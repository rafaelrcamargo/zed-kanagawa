{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Kanagawa Paper",
  "author": "Ethan Gilmore <ethanmckgilmore@gmail.com>",
  "themes": [
    {
      "name": "Kanagawa Paper",
      "appearance": "dark",
      "style": {
        // Title and status bars
        "title_bar.background": "#16161D",
        "status_bar.background": "#16161D",

        // Tab bar
        "tab_bar.background": "#16161D",
        "tab.inactive_background": "#16161D",
        "tab.active_background": "#1F1F28",
        "toolbar.background": "#16161D",

        // Editor
        "background": "#1F1F28",
        "editor.background": "#1F1F28",
        "editor.gutter.background": "#1F1F28",
        "editor.active_line.background": "#363646",
        "editor.foreground": "#DCD7BA",
        "editor.line_number": "#54546D",
        "editor.active_line_number": "#DCD7BA",
        "predictive": "#727169",

        // Other panels
        "panel.background": "#1F1F28",
        "elevated_surface.background": "#2A2A37",
        "search.match_background": "#363646",
        "element.background": "#1F1F28",
        "element.hover": "#363646",

        // Borders
        "border": "#363646",
        "border.variant": "#363646",

        // UI Text
        "text": "#DCD7BA",
        "text.muted": "#727169",
        "text.placeholder": "#727169",
        "text.disabled": "#54546D",
        "text.accent": "#FFA066",
        "icon": "#DCD7BA",
        "icon.muted": "#727169",
        "icon.placeholder": "#727169",
        "icon.disabled": "#54546D",
        "icon.accent": "#FFA066",

        // Diagnostics
        "error": "#ff5d62",
        "error.border": "#ff5d62",
        "error.background": "#ff5d62",
        "warning": "#FF9E3B",
        "warning.border": "#FF9E3B",
        "warning.background": "#FF9E3B",
        "info": "#658594",
        "info.border": "#658594",
        "info.background": "#658594",
        "hint": "#72716960",
        "hint.border": "#72716960",
        "hint.background": "#72716960",

        // Git
        "created": "#8a9a7b",
        "created.border": "#8a9a7b",
        "created.background": "#8a9a7b",
        "deleted": "#c4746e",
        "deleted.border": "#c4746e",
        "deleted.background": "#c4746e",
        "modified": "#c4b28a",
        "modified.border": "#c4b28a",
        "modified.background": "#c4b28a",
        "ignored": "#72716960",
        "ignored.border": "#72716960",
        "ignored.background": "#72716960",

        // Terminal
        "terminal.background": "#1F1F28",
        "terminal.foreground": "#DCD7BA",
        "terminal.ansi.black": "#0d0c0c",
        "terminal.ansi.red": "#c4746e",
        "terminal.ansi.green": "#8a9a7b",
        "terminal.ansi.yellow": "#c4b28a",
        "terminal.ansi.blue": "#8ba4b0",
        "terminal.ansi.magenta": "#a292a3",
        "terminal.ansi.cyan": "#8ea4a2",
        "terminal.ansi.white": "#C8C093",
        "terminal.ansi.bright_black": "#727169",
        "terminal.ansi.bright_red": "#E46876",
        "terminal.ansi.bright_green": "#87a987",
        "terminal.ansi.bright_yellow": "#E6C384",
        "terminal.ansi.bright_blue": "#7FB4CA",
        "terminal.ansi.bright_magenta": "#938AA9",
        "terminal.ansi.bright_cyan": "#7AA89F",
        "terminal.ansi.bright_white": "#c5c9c5",

        // Cursors
        "players": [
          {
            "cursor": "#DCD7BA",
            "selection": "#DCD7BA30"
          },
          {
            "cursor": "#c4b28a",
            "selection": "#c4b28a30"
          },
          {
            "cursor": "#8a9a7b",
            "selection": "#8a9a7b30"
          },
          {
            "cursor": "#c4746e",
            "selection": "#c4746e30"
          }
        ],

        // Syntax highlighting
        "syntax": {
          "attribute": {
            "color": "#b6927b",
            "font_style": null,
            "font_weight": null
          },
          "boolean": {
            "color": "#b6927b",
            "font_style": null,
            "font_weight": null
          },
          "comment": {
            "color": "#727169",
            "font_style": "italic",
            "font_weight": null
          },
          "constant": {
            "color": "#b6927b",
            "font_style": null,
            "font_weight": null
          },
          "constructor": {
            "color": "#8ba4b0",
            "font_style": null,
            "font_weight": null
          },
          "function": {
            "color": "#8ba4b0",
            "font_style": null,
            "font_weight": null
          },
          "keyword": {
            "color": "#c4746e",
            "font_style": null,
            "font_weight": null
          },
          "number": {
            "color": "#a292a3",
            "font_style": null,
            "font_weight": null
          },
          "operator": {
            "color": "#8ba4b0",
            "font_style": null,
            "font_weight": null
          },
          "property": {
            "color": "#c4b28a",
            "font_style": null,
            "font_weight": null
          },
          "punctuation": {
            "color": "#8992a7",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.bracket": {
            "color": "#DCD7BA",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.delimiter": {
            "color": "#DCD7BA",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.special": {
            "color": "#98948B",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.special.symbol": {
            "color": "#98948B",
            "font_style": null,
            "font_weight": null
          },
          "string": {
            "color": "#8a9a7b",
            "font_style": null,
            "font_weight": null
          },
          "string.escape": {
            "color": "#98948B",
            "font_style": null,
            "font_weight": null
          },
          "string.regex": {
            "color": "#98948B",
            "font_style": null,
            "font_weight": null
          },
          "string.special": {
            "color": "#98948B",
            "font_style": null,
            "font_weight": null
          },
          "string.special.symbol": {
            "color": "#98948B",
            "font_style": null,
            "font_weight": null
          },
          "tag": {
            "color": "#b6927b",
            "font_style": null,
            "font_weight": null
          },
          "title": {
            "color": "#8992a7",
            "font_style": null,
            "font_weight": 800
          },
          "type": {
            "color": "#8ea4a2",
            "font_style": null,
            "font_weight": null
          },
          "variable": {
            "color": "#DCD7BA",
            "font_style": null,
            "font_weight": null
          },
          "variable.special": {
            "color": "#E46876",
            "font_style": null,
            "font_weight": null
          }
        }
      }
    }
  ]
}
